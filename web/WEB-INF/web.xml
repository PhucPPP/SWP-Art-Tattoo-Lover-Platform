<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MainController</servlet-name>
        <servlet-class>Controller.MainController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchStudioController</servlet-name>
        <servlet-class>Controller.SearchStudioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewStudioInforController</servlet-name>
        <servlet-class>Controller.ViewStudioInforController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SortServiceStudioController</servlet-name>
        <servlet-class>Controller.SortServiceStudioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewStudioServiceController</servlet-name>
        <servlet-class>Controller.ViewStudioServiceController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookController</servlet-name>
        <servlet-class>Controller.BookController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainController</servlet-name>
        <url-pattern>/MainController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchStudioController</servlet-name>
        <url-pattern>/SearchStudioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewStudioInforController</servlet-name>
        <url-pattern>/ViewStudioInforController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SortServiceStudioController</servlet-name>
        <url-pattern>/SortServiceStudioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewStudioServiceController</servlet-name>
        <url-pattern>/ViewStudioServiceController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookController</servlet-name>
        <url-pattern>/BookController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>MainController</welcome-file>
    </welcome-file-list>
</web-app>
